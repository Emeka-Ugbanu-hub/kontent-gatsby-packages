{
  "name": "gatsby-source-kentico-cloud",
  "version": "3.0.0-beta1",
  "description": "Gatsby source plugin for Kentico Cloud",
  "main": "./gatsby-node.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --config jest.coverage.config.js ",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "babel src --out-dir ./ --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production && npm run build",
    "watch": "babel -w src --out-dir ./ --ignore **/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kentico/gatsby-source-kentico-cloud"
  },
  "keywords": [
    "kentico-cloud",
    "gatsby-source-plugin",
    "Kentico",
    "Kentico Cloud",
    "GatsbyJS",
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "Jan Lenoch <janl@kentico.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "change-case": "^3.1.0",
    "flatted": "^2.0.0",
    "gatsby": "^2.0.91",
    "kentico-cloud-delivery": "^5.3.1",
    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "rxjs": "^6.3.3"
  },
  "peerDependencies": {
    "gatsby": ">=2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-config-google": "^0.10.0",
    "eslint-plugin-node": "^7.0.1",
    "jest": "^23.6.0",
    "kentico-cloud-js-sdk-test-http-service": "0.0.1",
    "regenerator-runtime": "^0.12.1"
  }
}
